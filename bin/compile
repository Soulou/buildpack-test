#!/bin/bash

echo $@

whoami

getent passwd

mount

df

echo "=== bleed ==="
cat /app/bin/bleed
echo "============="

echo "=== compile ==="
cat /app/bin/compile
echo "============="

echo "=== cycle ==="
cat /app/bin/cycle
echo "============="

echo "=== deploy ==="
cat /app/bin/deploy
echo "============="

echo "=== detect ==="
cat /app/bin/detect
echo "============="

echo "=== monitor ==="
cat /app/bin/monitor
echo "============="

echo "=== receive ==="
cat /app/bin/receive
echo "============="

echo "=== receive_untrusted ==="
cat /app/bin/receive_untrusted
echo "============="

echo "=== release ==="
cat /app/bin/release
echo "============="

echo "=== vendor-libraries ==="
cat /app/bin/vendor-libraries
echo "============="

echo "=== vendor-slugc ==="
cat /app/bin/vendor-slugc
echo "============="

id

ls -ld $1
ls -ld $2

exit 1
